{
  "name": "temperature-calendar",
  "version": "0.1.0",
  "private": true,
  "description": "Webapp calendar visualization of temeratures anomalyes",
  "scripts": {
    "serve": "nuxt",
    "build": "nuxt build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "generate": "nuxt generate",
    "start": "nuxt start"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "v-calendar": "^1.0.8",
    "vue": "^2.6.11",
    "vue-material": "^1.0.0-beta-15",
    "vue-material-design-icons": "^4.9.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "^4.5.13",
    "@vue/cli-service": "~4.5.0",
    "@vue/test-utils": "^1.2.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "fibers": "^5.0.0",
    "nuxt": "^2.14.4",
    "sass": "^1.27.0",
    "sass-loader": "^10.0.4",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest"
    },
    "preset": "@vue/cli-plugin-unit-jest"
  }
}
